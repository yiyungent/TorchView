<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>0.1.1</Version>
    <Company>yiyun</Company>
    <Authors>yiyun</Authors>
    <Description>Hybrid App for Xamarin, Xamarin combined with Vue.js and more.</Description>
    <Copyright>Copyright (c) 2021-present yiyun</Copyright>
    <RepositoryUrl>https://github.com/yiyungent/TorchView</RepositoryUrl>
    <PackageLicenseUrl>https://github.com/yiyungent/TorchView/blob/main/LICENSE</PackageLicenseUrl>
    <PackageTags>Xamarin Vue.js Hybrid</PackageTags>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>bin\Release\netstandard2.0\TorchView.xml</DocumentationFile>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Xamarin.Forms" Version="4.8.0.1451" />
    <!--<PackageReference Include="Xamarin.Essentials" Version="1.5.3.2" />-->
  </ItemGroup>

</Project>
